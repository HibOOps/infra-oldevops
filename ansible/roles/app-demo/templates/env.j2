# App Demo Environment Variables
# Managed by Ansible - Do not edit manually

# Database
POSTGRES_DB={{ app_demo_db_name }}
POSTGRES_USER={{ app_demo_db_user }}
POSTGRES_PASSWORD={{ app_demo_db_password }}

# Backend
DATABASE_URL=postgresql://{{ app_demo_db_user }}:{{ app_demo_db_password | urlencode }}@postgres:5432/{{ app_demo_db_name }}
JWT_SECRET={{ app_demo_jwt_secret }}
NODE_ENV=production

# Frontend
NEXT_PUBLIC_API_URL=https://{{ app_demo_backend_domain }}
