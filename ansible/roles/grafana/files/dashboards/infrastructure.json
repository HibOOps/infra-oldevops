                  "text": "DOWN"
                },
                "1": {
                  "color": "green",
                  "index": 0,
                  "text": "UP"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto",
        "wideLayout": true,
        "orientation": "auto"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "up{instance=~\"$host.*\"}",
          "legendFormat": "{{ job }} - {{ instance }}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "title": "Nombre de Conteneurs",
      "type": "stat",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 16
